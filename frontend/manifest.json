{
    "manifest_version": 3,
    "name": "SJSU Events Viewer",
    "version": "1.0",
    "description": "Displays today's events from SJSU events page.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://127.0.0.1:5000/*"
    ],
    "action": {
      "default_popup": "popup.html"
      
    
    }
  }
  